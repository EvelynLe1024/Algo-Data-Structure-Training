class Solution:
    def rob(self, nums: List[int]) -> int:
        n = len(nums)
        if n == 0:
            return 0
        if n == 1:
            return nums[0]
        
        h = [0] * n
        h[0] = nums[0] 
        h[1] = max(nums[1], nums[0])
    
        for i in range(2, n):
            h[i] = max(nums[i] + h[i - 2], h[i - 1])
        return h[n - 1]
        
